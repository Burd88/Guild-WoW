             x:Class="Notes.Views.MemberPage"
             Title="Active player"
              BackgroundImageSource="background.png" >
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="UpdateButton" Text="Обновить"
                     Clicked="OnUpdateInfo"  />
        
    </ContentPage.ToolbarItems>
